
P = $linear {
		vary entity = $markov_brain {
			vary inputs = 5
			vary outputs = 2
			vary hiddens = 2
		}
		vary size =  100 
		vary track-lineage = true
}

rep-for = $replicate {
			vary env = $forager {
				vary  sensor-range = 5
				vary replace =  true 
				food-eaten -> x
				ist line-of-sight = in-sense
				ost action = out-sense
			}
			fx -> score
			vary num = 3
}


elite = $elite_replacement {
			vary strength = 0.5
			value <- score
		} 

roul = $roulette_wheel {
			vary strength = 0.5
			value <- score
		} 

E = $two_cycle {
		vary fitness = !rep-for
		vary count = 100 
		vary cout-frequency = 10
		vary snapshot-frequency = 50
		vary pop-avmax-frequency = 0
		vary selector = !elite
} 

