
mark = $markov_brain {
	 vary inputs = 4 
	 vary outputs = 2
	 vary hiddens =  3 
}

cppn =  $cppn {
	 vary inputs = 4 
	 vary outputs = 2
	 vary hiddens =  3 
	 vary recurr =  3 
}

rnn_cppn = $rnn_with_cppn {
	 vary inputs = 4 
	 vary outputs = 2
	 vary recurr  =  3
	 vary cppn-hidden =  3 
	 vary cppn-recurr =  2 
	 vary src-x =  0 
	 vary src-y =  0 
	 vary offset =  0.01 
}

rnn = $rnn {
	 vary inputs = 4 
	 vary outputs = 2
	 vary recurr  =  6
}

rep-for = $replicate {
			vary env = $forager {
				vary  sensor-range = 4
				vary replace =  true 
				vary direction = false 
			}
			vary num = 3
}

elite = $elite_replacement {
			vary strength = 0.4
		} 

P = $linear {
        vary entity = [ !mark !cppn  !rnn !rnn_cppn ]
		#vary entity = !rnn_cppn
		#vary entity = !mark
        vary size = 20
        vary track-lineage = true
}

E = $two_cycle {
		vary fitness = !rep-for
		vary count = 10 
		vary cout-frequency = 1 
		vary snapshot-frequency = 1
		vary pop-avmax-frequency = 1
		vary selector = !elite
} 

cs = $two_cycle {
		vary fitness = !rep-for
		vary count = 10 
		vary cout-frequency = 1 
		vary snapshot-frequency = 1
		vary pop-avmax-frequency = 1
		vary selector = !elite
} 

