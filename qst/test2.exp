

e = $markov_brain {
     hiddens = 5
	 outputs = 2
	 inputs = 4
	}

P = $linear { 
	size = 40
	entity = !e # `entity` is special
	track_lineage = true 
	}

rep_for = $replicate {
		num = 3
		env = $forager
	}

fs = 
  $sequence  {
     first 	= !rep_for 
     second = $elite_replacement { 
 	 strength = 	0.4 
	 #one>food_eaten = two<score
  }
}


E = $cycle  { 
 count = 100
  world = !fs
}

