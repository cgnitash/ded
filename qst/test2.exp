

e = $markov_brain {
     hiddens = 5
	 outputs = 2
	 inputs = 4
	}

P = $linear { 
	size = 40
	entity = !e # `entity` is special
	track_lineage = true 
	}

rep_for = $replicate {
		num = 3
		env = $forager
		? x 10 
	}

fs = 
  $sequence  {
     first 	= !rep_for 
     second = $elite_replacement { 
 	 strength = [ 0.1 0.25	0.4 ]  
  }
}


E = $cycle  { 
 count = 501
  world = !fs
}

