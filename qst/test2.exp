
# working
# tests nested process with nested tags 

e = $markov_brain {
     hiddens = 5
	 outputs = 2
	 inputs = 4
	}

Population = $linear 
	{ 
	size = 40
	entity = !e # `entity` is special
	track_lineage = true 
	}

rep_for = $replicate 
	{
		num = 2 
		env = $forager 
				{ 
				? food_eaten 3 
				action <- out_sense 
				line_of_sight ->  in_sense 
				}
		? x 10 
	}
		
fs = 
  $sequence  {
     first 	= !rep_for 
     second = $elite_replacement { 
 	 strength = 0.4   
  }
}


Process = $cycle  { 
 count = 101
  world = !fs
}

