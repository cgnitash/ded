

e = $markov_brain {
     hiddens = 5
	 outputs = 2
	 inputs = 4
	}

P = $linear { 
	size = 40
	entity = !e # `entity` is special
	track_lineage = true 
	}

dn = $sequence {
     first 	= 	$fork {
     	one = $forager
     	two = $forager 
			  { direction = false }
	}
     second = $smoosh 
}



fs = 
  $sequence  {
     first 	= !dn 
     second = $elite_replacement { 
 	 strength = 	0.4 
  }
}


E = $cycle  { 
 count = 100
  world = !fs
}

